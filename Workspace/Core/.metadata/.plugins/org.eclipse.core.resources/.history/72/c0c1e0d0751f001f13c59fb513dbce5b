package first;

public class Product {

	private int pid,quantity;
	private float price;
	public Product() {}
	public Product(int pid, int quantity, float price) {
		super();
		this.pid = pid;
		this.quantity = quantity;
		this.price = price;
	}
	public int getPid() {
		return pid;
	}
	public int getQuantity() {
		return quantity;
	}
	public float getPrice() {
		return price;
	}
	@Override
	public String toString() {
		return "Product [pid=" + pid + ", quantity=" + quantity + ", price=" + price + "]";
	}
	
	
}
